      ],
      "text/plain": [
       "          ⎛ 2⎞\n",
       "⎛     2⎞  ⎝r ⎠\n",
       "⎝1 - r ⎠⋅ℯ    \n",
       "──────────────\n",
       "            2 \n",
       "⎛      ⎛ 2⎞⎞  \n",
       "⎜ 2    ⎝r ⎠⎟  \n",
       "⎝r  + ℯ    ⎠  "
      ]
     },
     "execution_count": 6,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "Kn = dz * d2z\n",
    "Kd = r * (1 + dz**2)**2\n",
    "K = Kn / Kd\n",
    "K.simplify()"
   ]
  }
